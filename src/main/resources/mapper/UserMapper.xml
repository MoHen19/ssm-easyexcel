<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.mohen.mapper.UserMapper">

    <select id="getAll" resultType="User">
        SELECT
            id,
            name,
            age,
            birthday,
            score
        FROM user
    </select>


    <insert id="saveBatch">
        insert into user (name,age,birthday,score)
        values
        <foreach collection="list" separator="," item="user">
            (#{user.name},#{user.age},#{user.birthday},#{user.score})
        </foreach>
    </insert>

</mapper>